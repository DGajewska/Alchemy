{
  // Visit https://aka.ms/tsconfig to read more about this file
  "compilerOptions": {
    "outDir": "src/dist",
    "module": "commonjs",
    "target": "esnext",
    "sourceMap": true,
    "strict": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "noImplicitAny": true
  },
  "include": [
    "./*.js",
    "./src/**/*.ts",
    "./test/**/*.ts"
  ]
}
